<?xml version="1.0" encoding="UTF-8"?>
<edges xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/edges_file.xsd">
    <!-- Incoming edges (approaching junction) -->
    <edge id="north_in" from="north_end" to="center" numLanes="2" speed="13.89"/>
    <edge id="east_in" from="east_end" to="center" numLanes="2" speed="13.89"/>
