    <edge id="west_in" from="west_end" to="center" numLanes="2" speed="13.89"/>
    
    <!-- Outgoing edges (leaving junction) -->
    <edge id="north_out" from="center" to="north_end" numLanes="2" speed="13.89"/>
    <edge id="east_out" from="center" to="east_end" numLanes="2" speed="13.89"/>
    <edge id="south_out" from="center" to="south_end" numLanes="2" speed="13.89"/>
    <edge id="west_out" from="center" to="west_end" numLanes="2" speed="13.89"/>
